{
  "name": "eo-sdk-boilerplate",
  "version": "1.0.0",
  "config": {
    "resourcename": "custom"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config node_modules/@eo-sdk/client/proxy.config.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "postbuild": "npm run postbuild:prod",
    "postbuild:prod": "node node_modules/@eo-sdk/client/misc/scripts/preparedist && node node_modules/@eo-sdk/client/misc/scripts/zipdist",
    "update": "npm install -P --save-exact @eo-sdk/client@stable",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@eo-sdk/client": "5.2.0-rc.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "~6.1.5",
    "@angular/compiler-cli": "^6.1.0",
    "@angular/language-service": "^6.1.0",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "bestzip": "1.1.4",
    "codelyzer": "~4.2.1",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "replace-in-file": "~3.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~2.7.2"
  }
}
