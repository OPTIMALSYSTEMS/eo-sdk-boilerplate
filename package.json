{
  "name": "eo-sdk-boilerplate",
  "version": "1.0.0",
  "config": {
    "resourcename": "custom"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --proxy-config node_modules/@eo-sdk/client/proxy.config.json",
    "build": "ng build",
    "build:prod": "ng build --prod",
    "postbuild": "npm run postbuild:prod",
    "postbuild:prod": "node node_modules/@eo-sdk/client/misc/scripts/preparedist && node node_modules/@eo-sdk/client/misc/scripts/zipdist",
    "update": "npm install -P --save-exact @eo-sdk/client@stable",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@eo-sdk/client": "6.16.1",
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1002.2",
    "@angular-devkit/build-ng-packagr": "~0.1002.2",
    "@angular/cli": "10.2.2",
    "@angular/compiler-cli": "10.2.4",
    "@angular/language-service": "10.2.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "bestzip": "1.1.4",
    "codelyzer": "^5.1.2",
    "fs-extra": "^7.0.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "node-sass": "^4.13.1",
    "protractor": "~7.0.0",
    "replace-in-file": "~3.4.2",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.0.7"
  }
}
